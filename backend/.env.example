# FILE: /AutoStudio/backend/.env.example
# DESCRIPTION: Backend environment variables template
# USAGE: Copy to .env and fill in actual values

# ============================================
# APPLICATION
# ============================================
NODE_ENV=development
PORT=4000
API_URL=http://localhost:4000
FRONTEND_URL=http://localhost:3000

# ============================================
# DATABASE
# ============================================
DATABASE_URL="postgresql://user:password@localhost:5432/autostudio?schema=public"
# Railway example: postgresql://postgres:password@containers-us-west-xxx.railway.app:5432/railway

# ============================================
# REDIS
# ============================================
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
# Railway example: redis://:password@containers-us-west-xxx.railway.app:6379

# ============================================
# JWT & AUTHENTICATION
# ============================================
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d
REFRESH_TOKEN_SECRET=your-refresh-token-secret
REFRESH_TOKEN_EXPIRES_IN=30d

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:4000/api/auth/google/callback

# Facebook OAuth (optional)
FACEBOOK_APP_ID=your-facebook-app-id
FACEBOOK_APP_SECRET=your-facebook-app-secret
FACEBOOK_CALLBACK_URL=http://localhost:4000/api/auth/facebook/callback

# ============================================
# EMAIL (for notifications)
# ============================================
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
EMAIL_FROM=noreply@autostudio.ai

# Or use SendGrid
SENDGRID_API_KEY=your-sendgrid-api-key

# ============================================
# CLOUDFLARE R2 (Storage)
# ============================================
R2_ACCOUNT_ID=your-cloudflare-account-id
R2_ACCESS_KEY_ID=your-r2-access-key-id
R2_SECRET_ACCESS_KEY=your-r2-secret-access-key
R2_BUCKET_NAME=autostudio-videos
R2_PUBLIC_URL=https://videos.autostudio.ai
R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com

# ============================================
# AI SERVICES - TEXT-TO-VIDEO
# ============================================

# Runway Gen-2
RUNWAY_API_KEY=your-runway-api-key
RUNWAY_API_URL=https://api.runwayml.com/v1

# Stability AI
STABILITY_API_KEY=your-stability-api-key
STABILITY_API_URL=https://api.stability.ai/v1

# Veo 3 (Google DeepMind)
VEO3_API_KEY=your-veo3-api-key
VEO3_PROJECT_ID=your-google-cloud-project-id
VEO3_API_URL=https://veo.googleapis.com/v1

# ============================================
# AI SERVICES - TEXT GENERATION (PIE)
# ============================================

# OpenAI GPT-4
OPENAI_API_KEY=your-openai-api-key
OPENAI_ORG_ID=your-org-id
OPENAI_MODEL_SCRIPT=gpt-4-turbo
OPENAI_MODEL_IMAGE=dall-e-3

# Anthropic Claude
CLAUDE_API_KEY=your-claude-api-key
CLAUDE_MODEL=claude-3-5-sonnet-20250929

# Google Gemini
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-1.5-pro

# ============================================
# AI SERVICES - TEXT-TO-SPEECH
# ============================================

# ElevenLabs
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# OpenAI TTS (backup)
OPENAI_TTS_MODEL=tts-1-hd
OPENAI_TTS_VOICE=alloy

# ============================================
# AI SERVICES - MUSIC GENERATION
# ============================================

# Suno AI
SUNO_API_KEY=your-suno-api-key
SUNO_API_URL=https://api.suno.ai/v1

# ============================================
# PAYMENT GATEWAYS
# ============================================

# Stripe
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-stripe-webhook-secret
STRIPE_PRICE_STARTER=price_starter_id
STRIPE_PRICE_PRO=price_pro_id
STRIPE_PRICE_ENTERPRISE=price_enterprise_id

# MoMo
MOMO_PARTNER_CODE=your-momo-partner-code
MOMO_ACCESS_KEY=your-momo-access-key
MOMO_SECRET_KEY=your-momo-secret-key
MOMO_ENDPOINT=https://test-payment.momo.vn/v2/gateway/api/create
MOMO_RETURN_URL=http://localhost:3000/payment/momo/callback
MOMO_NOTIFY_URL=http://localhost:4000/api/webhooks/momo

# ZaloPay
ZALOPAY_APP_ID=your-zalopay-app-id
ZALOPAY_KEY1=your-zalopay-key1
ZALOPAY_KEY2=your-zalopay-key2
ZALOPAY_ENDPOINT=https://sb-openapi.zalopay.vn/v2/create
ZALOPAY_CALLBACK_URL=http://localhost:4000/api/webhooks/zalopay

# VNPay
VNPAY_TMN_CODE=your-vnpay-tmn-code
VNPAY_HASH_SECRET=your-vnpay-hash-secret
VNPAY_URL=https://sandbox.vnpayment.vn/paymentv2/vpcpay.html
VNPAY_RETURN_URL=http://localhost:3000/payment/vnpay/callback

# Crypto Payments (optional)
CRYPTO_WALLET_ADDRESS=your-btc-or-usdt-wallet-address
COINBASE_API_KEY=your-coinbase-api-key
COINBASE_WEBHOOK_SECRET=your-coinbase-webhook-secret

# ============================================
# BLOCKCHAIN - POLYGON
# ============================================
POLYGON_RPC_URL=https://polygon-rpc.com
POLYGON_CHAIN_ID=137
POLYGON_PRIVATE_KEY=your-polygon-wallet-private-key
POLYGON_CONTRACT_ADDRESS=your-deployed-contract-address
POLYGON_EXPLORER_URL=https://polygonscan.com

# For testnet (Mumbai)
# POLYGON_RPC_URL=https://rpc-mumbai.maticvigil.com
# POLYGON_CHAIN_ID=80001

# Arweave (optional for permanent storage)
ARWEAVE_KEY_FILE=./arweave-key.json
ARWEAVE_GATEWAY=https://arweave.net

# ============================================
# RATE LIMITING & SECURITY
# ============================================
RATE_LIMIT_TTL=60
RATE_LIMIT_LIMIT=100
CORS_ORIGINS=http://localhost:3000,https://autostudio.ai

# ============================================
# MONITORING & LOGGING
# ============================================
LOG_LEVEL=debug
SENTRY_DSN=your-sentry-dsn

# ============================================
# FEATURE FLAGS
# ============================================
ENABLE_AI_AUDIT=true
ENABLE_BLOCKCHAIN=true
ENABLE_COLLABORATION=true
ENABLE_ANALYTICS=true
ENABLE_CSH=true

# ============================================
# PRICING CONFIG (Credits per action)
# ============================================
CREDIT_VIDEO_5S=1
CREDIT_VIDEO_15S=3
CREDIT_VIDEO_30S=5
CREDIT_VIDEO_60S=10
CREDIT_VIDEO_PER_MINUTE=10
CREDIT_THUMBNAIL=1
CREDIT_REPROMPT_SHORT=1
CREDIT_REPROMPT_MEDIUM=2
CREDIT_REPROMPT_LONG=4
CREDIT_MUSIC_AI=2
CREDIT_POLICY_SCAN=1

# ============================================
# PLANS CONFIG
# ============================================
PLAN_FREE_CREDITS=10
PLAN_FREE_VIDEO_LIMIT=1
PLAN_STARTER_CREDITS=250
PLAN_STARTER_VIDEO_LIMIT=20
PLAN_STARTER_PRICE=19
PLAN_PRO_CREDITS=800
PLAN_PRO_VIDEO_LIMIT=100
PLAN_PRO_PRICE=49
PLAN_ENTERPRISE_CREDITS=5000
PLAN_ENTERPRISE_VIDEO_LIMIT=999999
PLAN_PARTNER_CREDITS=1500
PLAN_PARTNER_PRICE=99